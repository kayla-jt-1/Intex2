                }
            </div>

            <!--Display Night Dark Condition as Yes or No-->
            <div>
                @if (c.NIGHT_DARK_CONDITION == "true")
                {
                    <span>Night Dark Condition: Yes</span>
                }
                else
                {
                    <span>Night Dark Condition:  No </span>
