                    <span>Improper Restraint:  No </span>
                }
            </div>

            <!--Display Unrestrained as Yes or No-->
            <div>
                @if (c.UNRESTRAINED == "true")
                {
                    <span>Unrestrained: Yes</span>
                }
                else
                {
                    <span>Unrestrained:  No </span>
