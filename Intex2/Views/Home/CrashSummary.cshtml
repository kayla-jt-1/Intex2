            </div>

            <!--Display DUI as Yes or No-->
            <div>
                @if (c.DUI == "true")
                {
                    <span>DUI: Yes</span>
                }
                else
                {
                    <span>DUI:  No </span>
                }
            </div>

            <!--Display Intersection Related as Yes or No-->
